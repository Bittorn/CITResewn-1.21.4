{
  "schemaVersion": 1,
  "id": "citresewn",
  "version": "${mod_version}",
  "name": "CIT Resewn",
  "description": "Re-implements MCPatcher's CIT",
  "authors": [
    "SHsuperCM",
    "Bittorn"
  ],
  "license": "MIT",
  "icon": "assets/citresewn/logo.png",
  "contact": {
    "original": "https://citresewn.shcm.io/",
    "sources": "https://github.com/Bittorn/CITResewn-1.21.4",
    "issues": "https://github.com/Bittorn/CITResewn-1.21.4/issues"
  },

  "environment": "client",
  "entrypoints": {
		"client": [
			"shcm.shsupercm.fabric.citresewn.CITResewn"
		],
    
    "citresewn:condition": [
      "shcm.shsupercm.fabric.citresewn.cit.builtin.conditions.core.FallbackCondition",
      "shcm.shsupercm.fabric.citresewn.cit.builtin.conditions.core.WeightCondition"
    ],
    "citresewn:global_property": [
      "shcm.shsupercm.fabric.citresewn.cit.builtin.conditions.core.FallbackCondition"
    ],

    "modmenu": [
      "shcm.shsupercm.fabric.citresewn.config.CITResewnModMenu"
    ]
	},
  "accessWidener" : "citresewn.accesswidener",
  "mixins": [
    "citresewn.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "fabricloader": "*",
    "minecraft": "${mod_target_mc}"
  },
  "recommends": {
    "modmenu": "*",
    "cloth-config2": "*"
  },
  "breaks": {
    "canvas": "*"
  }
}
